@article{blaszczykNonstandardAnalysisCultural2016,
  title = {A Non-Standard Analysis of a Cultural Icon: {{The}} Case of {{Paul Halmos}}},
  shorttitle = {A Non-Standard Analysis of a Cultural Icon},
  author = {Blaszczyk, Piotr and Borovik, Alexandre and Kanovei, Vladimir and Katz, Mikhail G. and Kudryk, Taras and Kutateladze, Semen S. and Sherry, David},
  date = {2016-12},
