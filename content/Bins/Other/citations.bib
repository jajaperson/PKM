@unpublished{zhaoOpticalCavity2013,
  title = {Optical Cavity},
  author = {Zhao, Chunnong},
  date = {2013},
  location = {University of Western Australia},
  abstract = {This laboratory experiment explores the characteristics of the Gaussian beam and its propagation, a two-mirror optical cavity, and the coupling of the Gaussian beam to the optical cavity. The first portion of the experiment seeks to characterize the Gaussian beam from a He-Ne laser. The second part provides some experience in designing the mode matching system to couple the Gaussian beam into an optical cavity. The third part seeks to provide some experience in aligning two mirrors to form a cavity such that the input laser beam is reasonably well-coupled into the system. One of the cavity mirrors is scanned back and forth using a piezo element so that the length of the cavity changes in a periodic manner. An array of different spatial modes will be observed as the length is changed slowly. You may characterize the optical cavity by measuring its free spectral range and finesse.},
