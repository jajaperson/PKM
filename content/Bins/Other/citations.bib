  shorttitle = {Are {{Luxury Brand Labels}} and ‚Äú{{Green}}‚Äù {{Labels Costly Signals}} of {{Social Status}}?},
  author = {Berger, Jo√´l},
  date = {2017-02-07},
  journaltitle = {PLOS ONE},
